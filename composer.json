{
  "name": "execut/yii2-navigation",
  "description": "Widgets and components for simplify navigation ",
  "keywords": ["yii2"],
  "homepage": "https://github.com/execut/yii2-navigation",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Mamaev Yuriy (eXeCUT)"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "yiisoft/yii2": "@dev",
    "yiisoft/yii2-jui": "@dev"
  },
  "require-dev": {
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0"
  },
  "autoload": {
    "psr-4": {
      "execut\\navigation\\": "src"
    }
  },
  "extra": {
    "bootstrap": "execut\\navigation\\Bootstrap"
  }
}